<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//CN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-

transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=gb2312" />
<title>无标题文档</title>
</head>

<body>
<script type="text/javascript">
var supportsDOM2Core = document.implementation.hasFeature("Core","2.0");
var supportsDOM3Core = document.implementation.hasFeature("Core","3.0");
var supportsDOM2HTML = document.implementation.hasFeature("HTML","2.0");
var supportsDOM2Views = document.implementation.hasFeature("Views","2.0");
var supportsDOM2XML = document.implementation.hasFeature("XML","2.0");

document.write("是否支持DOM2级核心："+supportsDOM2Core);
document.write("是否支持DOM3级核心："+supportsDOM3Core);
document.write("是否支持DOM2级HTML："+supportsDOM2HTML);
document.write("是否支持DOM2级视图："+supportsDOM2Views);
document.write("是否支持DOM2级XML："+supportsDOM2XML);

alert("是否支持DOM2级核心："+supportsDOM2Core);
alert("是否支持DOM3级核心："+supportsDOM3Core);
alert("是否支持DOM2级HTML："+supportsDOM2HTML);
alert("是否支持DOM2级视图："+supportsDOM2Views);
alert("是否支持DOM2级XML："+supportsDOM2XML);

</script>
</body>
</html>
